  height: 31px;
  line-height: 31px;
}
.tag_appender_hidden_text {
  position: absolute;
  top: 100%;
}
.tag_delete {
  cursor: pointer;
  height: 28px;
  line-height: 28px;
  width: 18px;
  float: right;
  margin-right: 5px;
}
.tag_delete:before {
  content: "\f00d";
}
.tag_text {
  cursor: default;
  position: relative;
  float: left;
  margin-right: 5px;
  line-height: 28px;
  height: 28px;
  z-index: 1;
}

